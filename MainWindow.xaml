        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Borisin_glazki_save"
        mc:Ignorable="d"
        Title="Борисин" Height="450" Width="800" FontFamily="Century Gothic" Icon=" agents/Глазки-save.ico">
    <Grid>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="agents/Глазки-save.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text=" Глазки" FontSize=" 30" HorizontalAlignment="Center" VerticalAlignment="Center" ></TextBlock>
        <Frame Name="MainFrame" Grid.Row=" 1"  ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden" ></Frame>
        <Button Content=" Назад" Name="BtnBack"  HorizontalAlignment="Right" Click="BtnBack_Click"></Button>


    </Grid>
</Window>
